<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.pillartechnology.codemash</groupId>
  <artifactId>bytecode-dc</artifactId>
  <version>1-SNAPSHOT</version>
  <dependencies>
      <dependency>
          <groupId>org.ow2.asm</groupId>
          <artifactId>asm</artifactId>
          <version>5.2</version>
      </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
            <source>1.8</source>
            <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
